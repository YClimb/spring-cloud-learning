# PS：当前文件使用 consul 的自动配置实现，需要在 key/value 中创建
# PPS：key=config/consul-provider/data;value=当前整个yml文件的内容
server:
    port: 8001
spring:
    profiles:
        active: dev
    cloud:
        consul:
            discovery:
                tags: dev
                hostname: localhost
                # 服务发现过滤的tag
                default-query-tag: dev
                # 必须配置使用 ip 来区分服务
                prefer-ip-address: true
                instance-id: ${spring.application.name}:${spring.cloud.client.ip-address}
